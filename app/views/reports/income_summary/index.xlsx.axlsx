wb = xlsx_package.workbook
wb.add_worksheet(name: "Ingresos") do |sheet|
    cops = 0
    sheet.add_row ["Descripci√≥n", "Actividad", "Fecha", "Concepto"]
    @incomes.each do |income|
        sheet.add_row [income.descripcion, income.tipo, income.fecha_inicio, income.concepto]
        cops += income.concepto
    end
    sheet.add_row ["Total", "", "", cops]
end